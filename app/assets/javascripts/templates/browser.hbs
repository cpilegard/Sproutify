<h1>Browser</h1>
<h3>
  Supplies
</h3>
<ul>
{{#eachProperty supply}}
  <p>
    <input type="checkbox" class="supply-filter" data-name="{{property}}">  {{property}}: {{value.length}}
  </p>
{{/eachProperty}}
</ul>
<h3>
  Demands
</h3>
<ul>
{{#eachProperty demand}}
  <p>
    <input type="checkbox" class="demand-filter" data-name="{{property}}-demand"> {{property}}: {{value.length}}
  </p>
{{/eachProperty}}
</ul>