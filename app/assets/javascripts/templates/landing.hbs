<div class="container">
  <h1> Find Gardeners near you </h1>
  <form accept-charset="UTF-8" action="/" method="post" role="form">
    <div class="form-group">
      <input class="form-control" id="address" name="landing[location]" placeholder="Street Address" size="30" type="text">
    </div>
    <div class="form-group">
      <input class="form-control" id="zip" name="landing[location]" placeholder="Zip Code" size="30" type="text">
    </div>
    <button class="btn btn-primary" id="submit" name="commit" type="submit">Check</button>
  </form>
  <div class="user_count">
  </div>
  <div class="available">
  </div>
  <div class="demanded">
  </div>
  <p>
    <a href="#" class="btn btn-primary signup-link" data-remote="true">Signup</a>
  </p>

  <input id="hidden_lat" type="hidden">
  <input id="hidden_lng" type="hidden">

</div>
